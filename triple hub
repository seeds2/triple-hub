ToggleButton.Size = UDim2.new(0, 200, 0, 30)
ToggleButton.Position = UDim2.new(0.5, -100, 0, 120)
ToggleButton.Text = "Hitboxes: ON"
ToggleButton.BackgroundColor3 = Color3.fromRGB(0, 200, 100)
