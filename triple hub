    local player = game.Players.LocalPlayer
    local chatEvent = game:GetService("ReplicatedStorage"):WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest")
    task.wait(0.5) -- short delay to ensure GUI is visible
    chatEvent:FireServer("Triple Hub Has Been Executed!", "All")

	-- Send notification once after GUI loads
pcall(function()
